     "name": "stdout",
     "output_type": "stream",
     "text": [
      "True\n"
     ]
    }
   ],
   "source": [
    "mask = np.zeros_like(a).astype(bool)\n",
    "for b_i in b:\n",
    "    mask_i = (a == b_i)\n",
    "    mask = np.logical_or(mask, mask_i)\n",
    "    \n",
    "print(mask.sum()==b.size)"
   ]
  },
