   "outputs": [],
   "source": [
    "a = np.array([1, 2, 3, 4, 5])\n",
    "b = np.array([1, 3, 5])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 28,
   "id": "17eb4abc",
   "metadata": {},
   "outputs": [
